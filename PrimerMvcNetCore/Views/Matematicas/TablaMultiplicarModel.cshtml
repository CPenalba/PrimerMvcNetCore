@model List<FilaTablaMultiplicar> <h1 style="color:blue">Tabla multiplicar model</h1> <form method="post">    <label>Número</label>    <input type="text" name="numero"        class="form-control" />    <button class="btn btn-outline-warning">        Mostrar tabla    </button></form> @if (Model != null){    <table class="table table-bordered">        <thead>            <tr>
                <th>Operacion</th>
                <th>Resltado</th>
            </tr>
        </thead>        <tbody>            @foreach(FilaTablaMultiplicar fila in Model){
                <tr>
                    <td>@fila.Operacion</td>
                    <td>@fila.Resultado</td>
                </tr>
            }
        </tbody>
    </table>}
